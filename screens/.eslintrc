
{
 "extends": [
   "eslint:recommended",
   "plugin:prettier/recommended",
   "plugin:react/recommended",
   "prettier",
   "prettier/react"
 ],
 "plugins": ["prettier", "react"],
 "parserOptions": {
   "ecmaVersion": 2018,
   "sourceType": "module",
   "ecmaFeatures": {
     "jsx": true
   }
 },
 "env": {
   "es6": true,
   "browser": true,
   "node": true
 },
 "rules": {
   "prettier/prettier": [
     "error",
     {
       "singleQoute": true,
       "printWidth": 120
     },
     {
       "usePrettierrc": false
     }
   ],
   "no-console": "warn",
   "semi": 2,
   "no-undef": "warn",
   "parser": "babel-eslint"
 }
}